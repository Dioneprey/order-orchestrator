{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "entryFile": "src/infra/main",
  "sourceRoot": ".",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "prisma/generated/**/*",
        "outDir": "dist"
      }
    ]
  }
}